{
  "manifest_version": 3,
  "name": "Auto Fill Google Forms",
  "version": "1.0",
  "description": "Remplit automatiquement un Google Form",
  "author": {
    "name": "Nkwenti Fonkou Alain Sainclair"
  },
  "permissions": ["activeTab", "scripting", "storage"],
  "content_scripts": [
    {
      "matches": ["https://docs.google.com/forms/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icons/icon128.png"
  }
}
